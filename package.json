{
  "name": "pdf-convert",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "migrate-and-run": "yarn prisma generate && yarn run dev",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "copyPrismaOrigin": "cp ../Sevi-Exodus/prisma/schema.prisma ./prisma/schema.prisma",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "docker": "yarn copyPrismaOrigin && yarn prisma generate && docker-compose -f ./docker/docker-compose.yml up leviticus",
    "docker:dev": "yarn prisma generate && docker-compose -f ./docker/docker-compose.yml up leviticus",
    "docker:kibana": " docker-compose -f ./docker/docker-compose.yml up kibana",
    "docker:prod": "docker-compose -f ./docker/docker-compose-prod.yml up leviticusp",
    "docker:neo": "docker compose -f ./docker/docker-compose-neo.yml up",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.56.0",
    "@aws-sdk/client-textract": "^3.53.0",
    "@aws-sdk/s3-request-presigner": "^3.56.0",
    "@elastic/elasticsearch": "^8.1.0",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^3.10.0",
    "@types/multer": "^1.4.7",
    "apollo-server-core": "^3.6.6",
    "apollo-server-express": "^3.6.6",
    "arangojs": "^7.7.0",
    "aws-sdk": "^2.767.0",
    "date-fns": "^2.28.0",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-plugin-import": "^2.25.4",
    "google-spreadsheet": "^3.2.0",
    "graphql": "^16.3.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.5.5",
    "imap-simple": "^5.1.0",
    "libphonenumber-js": "^1.9.49",
    "neo4j-driver": "^4.4.4",
    "pdf-to-base64": "^1.0.3",
    "pdf2json": "^2.0.0",
    "pdf2table": "^0.0.2",
    "prisma": "^3.10.0",
    "prisma-nestjs-graphql": "^14.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tabula-js": "^1.0.1",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "maxWorkers": 1,
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/../.jest/setEnvVars.js"
    ]
  }
}